[MASTER]
fail-under=8.0
jobs=2

[MESSAGES CONTROL]
enable=W
disable=line-too-long, import-outside-toplevel, missing-module-docstring, too-few-public-methods, redefined-builtin, no-else-return

[REPORTS]
score=y
reports=n

[TYPECHECK]
generated-members=numpy.*, torch.*

[DESIGN]
max-attributes=25
max-args=10
max-locals=20